{% autoescape false %}
    [
    {% for entity in entities %}
        {
        "id": "{{ entity.id }}",
        "title": "{{ entity.nombreCorto|default('-') }}",
        "start": "{{ entity.fechaSalidaCamara is not null ? entity.fechaSalidaCamara|date('Y-m-d') : '-' }}",
        "fechaSalidaCamara": "{{ entity.fechaSalidaCamara is not null ? entity.fechaSalidaCamara|date('Y-m-d') : '-' }}",
        "colorBandeja": "{{ entity.colorBandeja|default('-') }}",
        "orden": "{{ entity.orden|default('-') }}",
        "idPedido": "{{ entity.idPedido|default('-') }}",
        "colorProducto": "{{ entity.colorProducto|default('-') }}",
        "cantidadBandejas": "{{ entity.cantidadBandejas|default('-') }}",
        "tipoProducto": "{{ entity.tipoProducto|default('-') }}",
        "producto": "{{ entity.producto|default('-') }}",
        "estado": "{{ entity.estado|default('-') }}",
        "colorEstado": "{{ entity.colorEstado|default('-') }}",
        "idEstado": "{{ entity.idEstado|default('-') }}",
        "camaraDestino": "{{ entity.camaraDestino|default('-') }}",
        "pasaCamara": "{{ entity.pasaCamara|default('-') }}",
        "codigoSobre": "{{ entity.codigoSobre|escape_json|default('-')|raw|default('-') }}",
        "cliente": "{{ entity.cliente|default('-') }}",
        "fechaSalidaCamaraReal": "{{ entity.fechaSalidaCamaraReal is not null ? entity.fechaSalidaCamaraReal|date('Y-m-d') : '-' }}",
        "href":  "{{ path( "salida_camara_show", { 'id': entity.id }) }}",
        "className": "{{ entity.className|default('fc-event-danger fc-event-solid-danger') }}"
        }
        {{ (loop.last ? '' : ',') }}
    {% endfor %}
    ]

{% endautoescape %}