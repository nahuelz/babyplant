<div class="col-md-4">
    <div class="d-flex align-items-center mb-10 bandejas">
        <div class="symbol symbol-40 symbol-light-primary mr-5">
            <span class="symbol-label">
                <span class="svg-icon svg-icon-xl svg-icon-primary">
                    <i class="fas fa-table icon-2x text-dark"></i>
                </span>
            </span>
        </div>
        <div class="d-flex flex-column font-weight-bold">
            <label class="text-dark text-hover-primary mb-1 font-size-lg">Bandejas Sembradas / Pedidas</label>
            <span class="text-muted">{{ entity.cantidadBandejasReales|default(entity.cantidadBandejasReales) }}(X{{ entity.tipoBandeja }}) / {{ entity.cantidadBandejasPedidas|default(entity.cantidadBandejasPedidas) }}(X{{ entity.tipoBandeja }})</span>
            <input type="hidden" id="cantidadBandejasValue" value="{{ entity.cantidadBandejasReales|default(entity.cantidadBandejasPedidas) }}">
        </div>
    </div>
</div>
