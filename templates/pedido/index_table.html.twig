{% autoescape false %}
    {
    "data": [
    {% for entity in entities %}
        [
        "{{ entity.id }}",
        "{{ entity.fechaCreacion is not null ? entity.fechaCreacion|date('d/m/Y') : '-' }}",
        "{{ entity.producto|default('-') }}",
        "{{ entity.cliente|default('-') }}",
        "{{ entity.cantidadBandejas|default('-') }}",
        "{{ entity.fechaSiembra is not null ? entity.fechaSiembra|date('d/m/Y') : '-' }}",
        "{{ entity.fechaEntrega is not null ? entity.fechaEntrega|date('d/m/Y') : '-' }}",
        {#"{{ entity.estado|default('-') }}",#}
        {
        "estado" : "{{ entity.estado|default('-') }}",
        "colorEstado" : "{{ entity.colorEstado|default('default') }}"
        },
        "{{ entity.ordenSiembra|default('-') }}",
        "{{ entity.mesada|default('-') }}",
        {
        "show" : " {{ path( "pedido_show", { "id": entity.id }) }}",
        {#{% if is_granted('ROLE_PEDIDOS_EDIT') %}
            ,"edit" : " {{ path( "matriculado_edit", { "id": entity.id }) }}"
            ,"estados" : " {{ path( "matriculado_historico_estados", { "id": entity.id }) }}"
            ,"estado_cuenta" : " {{ path( "matriculado_show_estado_cuenta", { "id": entity.id }) }}"
            ,"matriculado_historico" : " {{ path( "matriculado_historico", { "id": entity.id }) }}"
        {% endif %}#}
        }]{{ (loop.last ? '' : ',') }}
    {% endfor %}
    ]
    #}
{% endautoescape %}