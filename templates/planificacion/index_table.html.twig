{% autoescape false %}
    [
    {% for entity in entities %}
        {
        "id": "{{ entity.id }}",
        "title": "{{ entity.nombreCorto|default('-') }}",
        "start": "{{ entity.fechaSiembra is not null ? entity.fechaSiembra|date('Y-m-d') : '-' }}",
        "description": "{{ entity.descripcion|default('-') }}",
        "cliente": "{{ entity.cliente|default('-') }}",
        "producto": "{{ entity.nombreCompleto|default('-') }}",
        "cantidadTipoBandejabandeja": "{{ entity.cantidadTipoBandejabandeja|default('-') }}",
        "fechaSiembra": "{{ entity.fechaSiembra is not null ? entity.fechaSiembra|date('Y-m-d') : '-' }}",
        "className": "fc-event-danger fc-event-solid-danger"
        }
        {{ (loop.last ? '' : ',') }}
    {% endfor %}
    ]

{% endautoescape %}