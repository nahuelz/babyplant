{% set EstadoReserva = 'App\\Entity\\Constants\\ConstanteEstadoReserva' %}
{% set SIN_ENTREGAR = constant(EstadoReserva ~ '::SIN_ENTREGAR') %}
{% autoescape false %}
    {
    "data": [
    {% for entity in entities %}
        [
        "{{ entity.id }}",
        "{{ entity.razonSocial }}",
        "{{ entity.cuit }}",
        {
        "show": "{{ path('razonsocial_show',{id: entity.id}) }}",
        "edit": "{{ path('razonsocial_edit',{id: entity.id}) }}",
        "situacion_cliente": "{{ path('situacionempresa_show',{id: entity.id}) }}"
        }
        ]{{ (loop.last ? '' : ',') }}
    {% endfor %}
    ]
    }
{% endautoescape %}