{% autoescape false %}
    [
    {% for entity in entities %}
        {
        "id": "{{ entity.id }}",
        "colorBandeja": "{{ entity.colorBandeja }}",
        "colorProducto": "{{ entity.colorProducto }}",
        "orden": "{{ entity.orden|default('-') }}",
        "idPedido": "{{ entity.idPedido|default('-') }}",
        "observacion": "{{ entity.observacion|escape_json|default('-')|raw|default('-') }}",
        "observacionCamara": "{{ entity.observacionCamara|escape_json|default('-')|raw|default('-') }}",
        "title": "{{ entity.title|html_format|raw|default('-') }}",
        "start": "{{ entity.fechaSiembraPlanificacion is not null ? entity.fechaSiembraPlanificacion|date('Y-m-d') : '-' }}",
        "href":  "{{ path( "pedido_producto_siembra", { 'id': entity.id }) }}",
        "className": "{{ entity.className|default('fc-event-danger fc-event-solid-danger') }}"
        }
        {{ (loop.last ? '' : ',') }}
    {% endfor %}
    ]

{% endautoescape %}