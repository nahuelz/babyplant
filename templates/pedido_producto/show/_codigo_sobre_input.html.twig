{% if entity.codigoSobre is not null %}
    <div class="col-md-4">
        <div class="d-flex align-items-center mb-10 sobre">
            <div class="symbol symbol-40 symbol-light-primary mr-5">
            <span class="symbol-label">
                <span class="svg-icon svg-icon-xl svg-icon-primary">
                    <i class="fas fa-envelope icon-2x text-dark"></i>
                </span>
            </span>
            </div>
            <div class="d-flex flex-column font-weight-bold">
                <label class="text-dark text-hover-primary mb-1 font-size-lg">Codigo del sobre</label>
                <span class="text-muted">{{ entity.codigoSobre|default('-') }}</span>
            </div>
        </div>
    </div>
{% endif %}