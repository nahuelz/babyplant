{% for notificacion in notificaciones %}
<!--begin::Item-->
<a href="#" class="navi-item">
    <div class="navi-link notificacion-link notificacion_ver" data-id-notificacion="{{ notificacion.id }}" data-contenido-notificacion="{{ notificacion.contenido }}">
        <div class="navi-icon mr-2">
            <i class="flaticon2-line-chart text-success"></i>
        </div>
        <div class="navi-text">
            <div class="font-weight-bold">
                {{ notificacion.titulo|clear_format|u.truncate(60, false, '...') }}
            </div>
            <div class="text-muted">
                {{ notificacion.fechaCreacion|ago|capitalize }}
            </div>
        </div>
    </div>
</a>
{% endfor %}