        <div class="form-group prototype-mesada" data-prototype="{% apply escape %}{% include 'salida_camara/prototype.mesada.html.twig' with {'pedidoProductoMesada': form.mesada.vars.prototype} %}{% endapply %}">
            {% for pedidoProductoMesada in form.mesada %}
                {% include 'salida_camara/prototype.mesada.html.twig' %}
            {% endfor %}
        </div>
        <div class="row">
            <div class="col-md-12">
                <a href="#" class="btn btn-light-success btn-sm prototype-link-add-mesada tooltips"
